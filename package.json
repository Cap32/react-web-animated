{
  "name": "react-web-animated",
  "version": "0.0.0-alpha.7",
  "description": "Bring react-native Animated to web",
  "scripts": {
    "start": "npm run karma -- start",
    "test": "npm run karma -- start --single-run",
    "karma": "cross-env ANIMATED_TEST=true babel-node -- ./node_modules/.bin/karma",
    "prebuild": "rimraf ./dist ./lib && mkdirp ./dist ./lib",
    "build:cjs-legacy": "babel src -d lib/legacy",
    "build:cjs": "babel src -d lib",
    "build:umd": "webpack src dist/react-web-animated.js",
    "build:min": "webpack -p src dist/react-web-animated.min.js",
    "build": "npm run build:cjs && npm run build:umd && npm run build:min && npm run build:cjs-legacy",
    "prepublish": "npm run build"
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "README.md"
  ],
  "author": "cap32",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "cross-env": "^3.1.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-webpack": "^2.0.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "react": "15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.4",
    "webpack": "v2.1.0-beta.25"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "emit-lite": "^1.0.0",
    "fbjs": "^0.8.0",
    "lodash": "^4.0.0"
  }
}
